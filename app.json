{
  "expo": {
    "name": "Relaxer",
    "slug": "Relaxer",
    "version": "1.2.1",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash_black.png",
      "resizeMode": "contain",
      "backgroundColor": "#212121"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#313131"
      },
      "package": "com.phoenixcreation.Relaxer"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
